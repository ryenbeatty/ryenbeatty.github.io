---
import Layout from 'layouts/BaseLayout.astro'

import { getCollection } from 'astro:content'

const projects = await getCollection('project')
---

<Layout>
  <main>
    <h2>Projects</h2>
    {
      projects.map((p) => (
        <a href={`/project/${p.slug}`} data-astro-prefetch="load">
          {p.slug}
        </a>
      ))
    }
  </main>
</Layout>
